{
  "expo": {
    "name": "olympus",
    "slug": "olympus",
    "version": "5.3.1",
    "orientation": "portrait",
    "icon": "./src/constants/assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/constants/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00512C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/constants/assets/adaptive-icon.png",
        "backgroundColor": "#00512C"
      }
    },
    "web": {
      "favicon": "./src/constants/assets/favicon.png"
    }
  }
}
