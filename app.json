{
  "expo": {
    "name": "olympus",
    "slug": "olympus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./constants/assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./constants/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#025529"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIStatusBarStyle": "UIStatusBarStyleLightContent"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./constants/assets/adaptive-icon.png",
        "backgroundColor": "#025529"
      },
      "package": "com.sedriast.olympus",
      "notification": {
        "color": "#025529"
      },
      "statusBar": {
        "backgroundColor": "#025529",
        "barStyle": "light-content"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./constants/assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ac5e3176-f1e1-4d66-bda7-70992754fa8f"
      }
    }
  }
}